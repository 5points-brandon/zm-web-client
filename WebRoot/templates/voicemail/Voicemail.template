								</tr>
								<tr>
									<td>
										<div id='${id}_voiceAnsweringLocale'></div>
									</td>
								</tr>
							</table>
						</td>
					</tr>

					<!-- User language
					<tr>
						<td class='ZOptionsLabelTop' style='width:200px'>
							<nobr>
								<$= ZmMsg.voicemailUserLocale $>
							</nobr>
						</td>
						<td class='ZOptionsField'>
							<table cellpadding='0' cellspacing='0'>
								<tr>
									<td>
										<$= ZmMsg.voicemailUserLocalePrompt $>
									</td>
								</tr>
								<tr>
									<td>
										<div id='${id}_voiceUserLocale'></div>
									</td>
								</tr>
							</table>
						</td>
					</tr>
					-->

					<tr>
						<td colspan='2'>
							<hr/>
						</td>
					</tr>

					<tr>
						<td class='ZOptionsLabelTop' style='width:200px'>
							<nobr>
								<$= ZmMsg.voicemailMessagePlayback $>
							</nobr>
						</td>
						<td class='ZOptionsField'>
							<table cellspacing='0' cellpadding='0'>
								<tr>
									<td>
										<div id='${id}_voiceAutoplayCheckbox'></div>
									</td>
								</tr>
								<tr>
									<td>
										<table cellspacing='0' cellpadding='0' style='padding-left:10px'>
											<tr>
												<td>
													<$= ZmMsg.voicemailPromptLevel $>
												</td>
											</tr>
											<tr>
												<td>
													<div id='${id}_voicePromptLevel'></div>
												</td>
											</tr>
											
										</table>
									</td>
								</tr>
								<tr>
									<td>
										<div id='${id}_voiceAnnounceDateTimeCheckbox'></div>
									</td>
								</tr>
							</table>
						</td>
					</tr>
					<tr>
						<td colspan='2'>
							<hr/>
						</td>
					</tr>

					<tr valign='top'>
						<td class='ZOptionsLabel' style='width:200px'>
							<nobr>
								<$= ZmMsg.voicePageSizeLabel $>
							</nobr>
						</td>
						<td class='ZOptionsField'>
							<table cellspacing='1' cellpadding='1'>
